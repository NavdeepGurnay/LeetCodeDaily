// Brute force and optimized Solution
// TC - O(n)  SC - O(1)

int finalValueAfterOperations(vector<string>& operations) {
    int X = 0;
    for (auto &op : operations) {
        if (op == "++X" || op == "X++") {
            X += 1;
        } else if (op == "--X" || op == "X--") {
            X -= 1;
        }
    }
    return X;
}
